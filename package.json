{
  "name": "react-unit-test-practice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dashboard -- webpack-dev-server --inline --progress",
    "prettier": "prettier --single-quote --trailing-comma all --write \"./src/**/*.js\" \"./webpack.config.js\"",
    "test": "ava",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test nyc ava",
    "report": "nyc report --reporter=html"
  },
  "ava": {
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [],
  "author": "Ryan Walsh <ryan@thehatrack.net> (https://twitter.com/_rtwalsh)",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.1",
    "chai-enzyme": "^0.8.0",
    "cross-env": "^5.0.3",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.7",
    "nyc": "^11.1.0",
    "prettier": "^1.5.3",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.0.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.4.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.6.1"
  }
}
